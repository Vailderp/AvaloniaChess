    <system:String x:Key="MlChangeAppLanguage" xml:space="preserve">App's language</system:String>
    <system:String x:Key="MlChangeAppDesign" xml:space="preserve">Color style</system:String>
    <system:String x:Key="MlChangeAppChessDesign" xml:space="preserve">Chess figures style</system:String>
    <system:String x:Key="MlChangeAppChessboardDesign" xml:space="preserve">Chessboard style</system:String>
